{
  "name": "zombie-char-component",
  "version": "1.0.7",
  "private": false,
  "author": {
    "name": "Mahdad Ghasemian",
    "email": "mahdad.ghasemian@gmail.com"
  },
  "repository": "https://github.com/MahdadGhasemian/zombie-char-component",
  "bugs": "https://github.com/MahdadGhasemian/zombie-char-component/issues",
  "keywords": [
    "zombie",
    "vue",
    "character",
    "solidity",
    "crypto"
  ],
  "license": "MIT",
  "description": "Vue Component for Zombie Character",
  "main": "dist/zombie-char-component.ssr.js",
  "browser": "dist/zombie-char-component.esm.js",
  "module": "dist/zombie-char-component.esm.js",
  "unpkg": "dist/zombie-char-component.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "big-integer": "^1.6.49",
    "js-sha3": "^0.8.0",
    "zombie-char-component": "^1.0.7"
  }
}
